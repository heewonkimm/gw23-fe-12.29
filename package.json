{
  "name": "gw23-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development export PORT=4000 && next dev",
    "build": "next build",
    "start": "next start",
    "build:local": "env-cmd -f .env.local next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:local": "env-cmd -f .env.local next build && export PORT=4000 && next start",
    "start:dev": "env-cmd -f .env.development next build && export PORT=4000 && next start",
    "start:prod": "env-cmd -f .env.production next build && export PORT=4000 && next start",
    "deploy:local": "env-cmd -f .env.local next build && pm2 startOrRestart ecosystem.config.js",
    "deploy:dev": "env-cmd -f .env.development next build && pm2 startOrRestart ecosystem.config.js",
    "deploy:prod": "env-cmd -f .env.production next build && pm2 startOrRestart ecosystem.config.js",
    "lint": "next lint",
    "storybook": "storybook dev -p 4010 --no-open",
    "build-storybook": "storybook build -o dist/storybook"
  },
  "browserslist": [
    "defaults"
  ],
